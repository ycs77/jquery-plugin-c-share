{
  "name": "jquery-plugin-c-share",
  "version": "1.1.0",
  "description": "The share to community buttons.",
  "main": "dist/jquery.c-share.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup -c",
    "compress": "uglifyjs dist/jquery.c-share.js -o dist/jquery.c-share.min.js -c -m --comments /^!/",
    "dev": "cross-env NODE_ENV=development rollup -c",
    "release": "npm run build && npm run compress",
    "watch": "rollup -c -w"
  },
  "author": "Lucas, Yang",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ycs77/jquery-plugin-c-share.git"
  },
  "homepage": "https://github.com/ycs77/jquery-plugin-c-share",
  "bugs": {
    "url": "https://github.com/ycs77/jquery-plugin-c-share/issues"
  },
  "keywords": [
    "jquery",
    "plugin",
    "jquery-plugin",
    "share",
    "share-buttons",
    "community"
  ],
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "change-case": "^4.1.1",
    "create-banner": "^1.0.0",
    "cross-env": "^7.0.2",
    "rollup": "^2.0.6",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "uglify-js": "^3.8.0"
  },
  "peerDependencies": {
    "jquery": "^3.4.1"
  }
}
